<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <div>
      <%  photos.forEach(function(element){ %>
        <div><%= element.name %></div>
        <div><%= element.description %></div>
        <div><%= element.location %></div>
        <%})%>
    </div>
  </body>
</html>
